<TEST_FLOW>

    <TEST TYPE = 'Normal' TEST-SUITE = 'Basic Call Flow' TEST-SHEET = 'Sanity' TEST-ID = 'SS-1' 
                DESCRIPTION = ' 401 for First Registration,
                                            Successful Second Registration,
                                            Successful De-Registration' >

		<ACTION SEND ='req' value = 'REGISTER' >
			<PARAM type = 'uri' value = 'Test1@127.0.0.1' /> 
			<PARAM type = 'password' value = 'Test' /> 
			<PARAM type = 'expires' value = '3600' /> 
			<PARAM type = 'serverport' value = '5070' /> 
			<PARAM type = 'proxyport' value = '5070' />
		</ACTION>
	<ACTION RECV='res' value='REGISTERED' />
	<ACTION SEND = 'req' value = 'DE-REGISTER' />
	<ACTION RECV = 'res' value = 'DE-REGISTERED'/>
	<WAIT unit = 'msec' value = '5000'/>

    </TEST>


    <TEST TYPE = 'Normal' TEST-SUITE = 'Basic Call Flow' TEST-SHEET = 'Sanity' TEST-ID = 'SS-2' 
                DESCRIPTION = ' Successful Registration,
                                            Session Establishment by Server Sending INVITE,
                                            Session Termination by Client Sending BYE,
                                            Successful De-registration' >

		<ACTION SEND ='req' value = 'REGISTER' >
			<PARAM type = 'uri' value = 'Test1@127.0.0.1' /> 
		</ACTION>
	<ACTION RECV='res' value='REGISTERED' />
        <ACTION RECV = 'req' value = 'INVITE' />
        <ACTION SEND = 'res' value= 'ACCEPT' />
	<ACTION RECV = 'res' value = 'SESSION_CONNECTED' />
	<ACTION SEND = 'req' value = 'BYE' />
	<ACTION RECV = 'res' value = 'SESSION_DISCONNECTED' />
	<WAIT unit = 'msec' value = '6000'/>
	<ACTION SEND = 'req' value = 'DE-REGISTER' />
	<ACTION RECV = 'res' value = 'DE-REGISTERED'/>
	<WAIT unit = 'msec' value = '5000'/>

    </TEST>


    <TEST TYPE = 'Normal' TEST-SUITE = 'Basic Call Flow' TEST-SHEET = 'Sanity' TEST-ID = 'SS-3' 
                DESCRIPTION = ' Successful Registration,
                                            Session Establishment by Client Sending INVITE,
                                            Session Termination by Server Sending BYE,
                                            Successful De-registration' >

		<ACTION SEND ='req' value = 'REGISTER' >
			<PARAM type = 'uri' value = 'Test1@127.0.0.1' /> 
		</ACTION>
	<ACTION RECV='res' value='REGISTERED' />
        <ACTION SEND = 'req' value = 'INVITE' >
            <PARAM type = 'uri' value =  'velu' />
	</ACTION>
	<ACTION RECV = 'res' value = 'SESSION_CONNECTED' />
        <ACTION RECV = 'res' value = 'SESSION_DISCONNECTED' />
	<WAIT unit = 'msec' value = '6000'/>
	<ACTION SEND = 'req' value = 'DE-REGISTER' />
	<ACTION RECV = 'res' value = 'DE-REGISTERED'/>
	<WAIT unit = 'msec' value = '5000'/>

    </TEST>


    <TEST TYPE = 'Normal' TEST-SUITE = 'Basic Call Flow' TEST-SHEET = 'Sanity' TEST-ID = 'SS-4' 
                DESCRIPTION = ' Successful Registration,
                                            Server Receiving INVITE,
                                            Server Receiving CANCEL,
                                            Successful De-registration' >

		<ACTION SEND ='req' value = 'REGISTER' >
			<PARAM type = 'uri' value = 'Test1@127.0.0.1' /> 
		</ACTION>
	<ACTION RECV='res' value='REGISTERED' />
	<ACTION SEND = 'req' value = 'INVITE' >
            <PARAM type = 'uri' value =  'velu' />
	</ACTION>
	<ACTION RECV = 'res' value= 'RINGING' />
	<ACTION SEND = 'req' value = 'CANCEL' />
	<ACTION RECV = 'res' value = 'SESSION_DISCONNECTED' />
	<WAIT unit = 'msec' value = '6000'/>
	<ACTION SEND = 'req' value = 'DE-REGISTER' />
	<ACTION RECV = 'res' value = 'DE-REGISTERED'/>
	<WAIT unit = 'msec' value = '5000'/>

    </TEST>

    <TEST TYPE = 'Normal' TEST-SUITE = 'Basic Call Flow' TEST-SHEET = 'Sanity' TEST-ID = 'SS-5' 
                DESCRIPTION = ' Successful Registration,
                                            Server Sending INVITE,
                                            Client Sending 486_INVITE response,
                                            Successful De-registration' >

		<ACTION SEND ='req' value = 'REGISTER' >
			<PARAM type = 'uri' value = 'Test1@127.0.0.1' /> 
		</ACTION>
		<ACTION RECV='res' value='REGISTERED' />
                <ACTION RECV = 'req' value = 'INVITE' />
                <ACTION SEND = 'res' value = 'REJECT' />
                <ACTION RECV = 'res' value = 'SESSION_DISCONNECTED' />
		<WAIT unit = 'msec' value = '6000'/>
		<ACTION SEND = 'req' value = 'DE-REGISTER-ALL' />
		<ACTION RECV = 'res' value = 'DE-REGISTERED'/>
		<WAIT unit = 'msec' value = '5000'/>
</TEST>
</TEST_FLOW>
